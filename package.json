{
    "name": "diannex-js",
    "version": "1.0.0",
    "description": "",
    "main": "./dist/diannex.js",
    "scripts": {
        "build": "npm-run-all --parallel build:*",
        "build:core-cjs": "esbuild ./index.ts --bundle --minify --outfile=./dist/diannex.js --platform=node --format=cjs --allow-overwrite",
        "build:core-esm": "esbuild ./index-web.ts --bundle --minify --outfile=./dist/diannex.mjs --format=esm --allow-overwrite"
    },
    "exports": {
        ".": {
            "node": "./dist/diannex.js",
            "require": "./dist/diannex.js",
            "browser": "./dist/diannex.mjs",
            "import": "./dist/diannex.mjs",
            "default": "./dist/diannex.mjs"
        }
    },
    "keywords": [
        "diannex"
    ],
    "author": "MadCreativity",
    "license": "MIT",
    "dependencies": {
        "npm-run-all": "^4.1.5",
        "esbuild": "^0.12.9",
        "@types/node": "^16.0.0",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "eslint": "^7.27.0",
        "typescript": "^4.3.2"
    },
    "devDependencies": {}
}
